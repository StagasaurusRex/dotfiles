Host freenas
 HostName freenas.local
 User root
 AddKeysToAgent yes
 UseKeychain yes
 IdentityFile ~/.ssh/id_rsa

Host wol-prd
 HostName www02.prd.aws.workoutloud.com
 User ubuntu
 AddKeysToAgent yes
 UseKeychain yes
 IdentityFile ~/.ssh/wol-admin

Host old-stg 
 Hostname 18.209.41.250 
 User ubuntu
 AddKeysToAgent yes
 UseKeychain yes
 IdentityFile ~/.ssh/wol-admin

Host wol-stg 
 User ubuntu
 StrictHostKeyChecking no
 ProxyCommand sh -c "aws ssm start-session --target $(aws ec2 describe-instances --filters "Name=tag:Name,Values=wol-stg-www01" "Name=instance-state-name,Values=running" --output text --query 'Reservations[*].Instances[*].InstanceId') --document-name AWS-StartSSHSession --parameters 'portNumber=%p'"

Host new-prd 
 User ubuntu
 StrictHostKeyChecking no
 ProxyCommand sh -c "aws ssm start-session --target $(aws ec2 describe-instances --filters "Name=tag:Name,Values=wol-prd-www01" "Name=instance-state-name,Values=running" --output text --query 'Reservations[*].Instances[*].InstanceId') --document-name AWS-StartSSHSession --parameters 'portNumber=%p'"

Host wol-gobi
 User ubuntu
 StrictHostKeyChecking no
 ProxyCommand sh -c "aws ssm start-session --target $(aws ec2 describe-instances --filters "Name=tag:Name,Values=wol-prd-www01" "Name=instance-state-name,Values=running" --output text --query 'Reservations[*].Instances[*].InstanceId') --document-name AWS-StartSSHSession --parameters 'portNumber=%p'"

Host *
 AddKeysToAgent yes
 UseKeychain yes
 IdentityFile ~/.ssh/id_rsa

